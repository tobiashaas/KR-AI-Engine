    "resolution_strategies": {
      "exact_match": {
        "name": "Exact Match",
        "description": "Try to match exact model numbers first",
        "priority": 1,
        "use_case": "When specific models are mentioned"
      },
      "placeholder_expansion": {
        "name": "Placeholder Expansion", 
        "description": "Expand placeholders to actual model ranges",
        "priority": 2,
        "use_case": "When placeholders like Cxx0i are used"
      },
      "series_inference": {
        "name": "Series Inference",
        "description": "Infer models from series information",
        "priority": 3,
        "use_case": "When only series is mentioned"
      },
      "database_lookup": {
        "name": "Database Lookup",
        "description": "Look up actual models from database",
        "priority": 4,
        "use_case": "When placeholders need to be resolved to actual models"
      }
    },
    "processing_rules": {
      "placeholder_detection": {
        "rules": [
