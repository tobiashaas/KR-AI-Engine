{
  "model_placeholder_patterns": {
    "description": "Patterns for handling model placeholders and wildcards in technical documentation",
    "placeholder_types": {
      "numeric_wildcards": {
        "name": "Numeric Wildcards",
        "description": "Patterns where numbers are replaced with 'x' or similar",
        "examples": [
          {
            "placeholder": "Cxx0i",
            "pattern": "C\\d{3}0i",
            "actual_models": ["C451i", "C550i", "C650i"],
            "manufacturer": "konica_minolta",
            "series": "i-series"
          },
          {
            "placeholder": "Cxx1i", 
            "pattern": "C\\d{3}1i",
            "actual_models": ["C451i", "C551i", "C651i"],
            "manufacturer": "konica_minolta",
            "series": "i-series"
          },
          {
            "placeholder": "Cxxx",
            "pattern": "C\\d{3}",
            "actual_models": ["C450", "C550", "C650"],
            "manufacturer": "konica_minolta",
            "series": "c-series"
          }
        ]
      },
      "letter_wildcards": {
        "name": "Letter Wildcards", 
        "description": "Patterns where letters are replaced with 'x' or similar",
        "examples": [
          {
            "placeholder": "HP LaserJet Pro xxx",
            "pattern": "HP LaserJet Pro \\w+",
            "actual_models": ["HP LaserJet Pro 400", "HP LaserJet Pro 500"],
            "manufacturer": "hp",
            "series": "laserjet_pro"
          },
          {
            "placeholder": "Lexmark CSxxx",
            "pattern": "Lexmark CS\\d+",
            "actual_models": ["Lexmark CS725", "Lexmark CS820"],
            "manufacturer": "lexmark",
            "series": "cs_series"
          }
        ]
      },
      "range_patterns": {
        "name": "Range Patterns",
        "description": "Patterns indicating ranges of models",
        "examples": [
          {
            "placeholder": "C450i-C650i",
            "pattern": "C(\\d{3})i-C(\\d{3})i",
            "actual_models": ["C450i", "C500i", "C550i", "C600i", "C650i"],
            "manufacturer": "konica_minolta",
            "series": "i-series"
          },
          {
            "placeholder": "HP 400-600 Series",
            "pattern": "HP (\\d{3})-(\\d{3}) Series",
            "actual_models": ["HP 400", "HP 500", "HP 600"],
            "manufacturer": "hp",
            "series": "laserjet"
          }
        ]
      },
      "series_placeholders": {
        "name": "Series Placeholders",
        "description": "Patterns indicating entire series",
        "examples": [
          {
            "placeholder": "i-series (bizhub Cxx0i/Cxx1i)",
