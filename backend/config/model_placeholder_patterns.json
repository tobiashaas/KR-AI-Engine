            "actual_models": ["C450i", "C451i", "C550i", "C551i", "C650i", "C651i"],
            "manufacturer": "konica_minolta",
            "series": "i-series"
          },
          {
            "placeholder": "DeskJet 2000 Series",
            "pattern": "DeskJet 2000 Series",
            "actual_models": ["DeskJet 2130", "DeskJet 2132", "DeskJet 2134"],
            "manufacturer": "hp",
            "series": "deskjet_2000"
          }
        ]
      }
    },
